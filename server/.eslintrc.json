{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": "standard",
  "overrides": [
  ],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "quotes": ["warn", "single"],
    "semi": ["warn", "always"],
    "comma-dangle": ["warn", "always-multiline"]
  }
}
