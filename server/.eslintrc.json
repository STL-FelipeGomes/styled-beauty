{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["standard"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "semi": ["error", "always"],
    "quotes": ["error", "single"],
    "comma-dangle": ["error", "only-multiline"],
    "space-before-function-paren": [
      "error",
      { "named": "never", "asyncArrow": "always" }
    ],
    "camelcase": "off"
  }
}
